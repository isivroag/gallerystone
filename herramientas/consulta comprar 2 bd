SELECT table_name, table_type
FROM information_schema.tables
WHERE table_schema = 'tecniemc_checatec'
  AND (table_type = 'BASE TABLE' OR table_type = 'VIEW')
  AND table_name NOT IN (
    SELECT table_name
    FROM information_schema.tables
    WHERE table_schema = 'tecniemc_checa'
      AND (table_type = 'BASE TABLE' OR table_type = 'VIEW')
  )